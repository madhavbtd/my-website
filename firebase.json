{
  "functions": [
    {
      "source": "agentfunctions",
      "codebase": "agentfunctions",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "runtime": "nodejs18",
      "region": "us-central1" // रीजन को us-central1 में बदला गया है
    }
    // आपके अन्य कोडबेस (default, stockfunctions) को अस्थायी रूप से हटा दिया गया है
    // ताकि डिप्लॉयमेंट केवल agentfunctions पर केंद्रित हो।
    // आप उन्हें बाद में वापस जोड़ सकते हैं जब वे ठीक से कॉन्फ़िगर हो जाएं।
  ],
  "hosting": {
    "public": "public", // यदि आप Firebase होस्टिंग का उपयोग कर रहे हैं, तो यह आपके सार्वजनिक फ़ोल्डर का नाम होना चाहिए
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
    // यदि आपके पास rewrites या redirects हैं, तो वे यहाँ रहेंगे
  }
}